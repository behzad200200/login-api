'use strict';

angular.module('myApp.view1', ['ngRoute'])

.config(['$routeProvider', function($routeProvider) {
  $routeProvider.when('/view1', {
    templateUrl: '<%= asset_path("users/view1.html") %>',
    controller: 'View1Ctrl'
  });
}])

.controller('View1Ctrl', ['$scope', '$auth', '$http', function($scope, $auth, $http) {
        $scope.registrationLogin = {};
        $scope.user_attributes = {}
        $scope.register = function(registrationLogin, user_attributes){
            $http({
                method: 'POST',
                url: '/contractors.json',
                data: {'contractor': registrationLogin,
                        'user': user_attributes
                }
            })
                .success(function (res){
                    console.log("yessss")
                })
                .error(function (res) {
                    console.log(res.data)
                })
        }

}]);